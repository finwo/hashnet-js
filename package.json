    "asymmetric-diffie-hellman": "0.0.9",
    "body-parser": "^1.19.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "http": "0.0.0",
    "minimist": "^1.2.2",
    "node-fetch": "^2.6.0",
    "simple-peer": "^9.6.2",
    "url-parse": "^1.4.7",
    "wrtc": "^0.4.4"
  },
  "devDependencies": {
    "@purtuga/esm-webpack-plugin": "^1.2.1",
